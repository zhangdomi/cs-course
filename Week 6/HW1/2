#include <stdio.h>
#include <stdlib.h>

int main()
{
    int array[10]={25, 69, 54, 8, 77, 6, 29, 10, 3, 98};
    int min = array[0];
    for(int i=0; i<10;i++){
        if(min>array[i]){
            min = array[i];
        }
    }
    for(int j=0; j<10; j++){
        printf("%d ", array[j]);
        if(array[j]== min)
            printf("min ");
    }
    return 0;
}
